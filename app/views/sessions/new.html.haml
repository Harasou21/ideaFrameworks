= form_with(url: login_path, scope: :session) do |f|
  - flash.each do |message_type,message|
    = message

  %div
    = f.label :email
    = f.text_field :email

  %div
    = f.label :password
    = f.password_field :password

  = f.submit "ログイン",name: 'login'

%p アカウント登録がまだですか？
= link_to "ユーザー登録", new_user_path
